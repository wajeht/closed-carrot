{
  "name": "closed-carrot-amarillos-tech-hub-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js",
    "dev:serve": "concurrently --kill-others --names \"REACT,EXPRESS\" -c \"bgCyan.black,bgGreen.black\" \"vite build --watch --mode development\" \"npm run dev:api\"",
    "dev": "concurrently --kill-others --names \"REACT,EXPRESS\" -c \"bgCyan.black,bgGreen.black\" \"vite\" \"npm run dev:api\"",
    "dev:ui": "vite",
    "dev:api": "nodemon ./src/server.js",
    "build": "npm run build:api && npm run build:ui",
    "build:api": "echo api does not need to build!",
    "build:ui": "vite build",
    "preview": "vite preview",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@adminjs/express": "^5.1.0",
    "@adminjs/mongoose": "^3.0.3",
    "@sentry/node": "^7.74.0",
    "@tanstack/react-router": "^0.0.1-beta.48",
    "@vitejs/plugin-react": "^3.1.0",
    "adminjs": "^6.8.7",
    "autoprefixer": "^10.4.16",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-formidable": "^1.2.0",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "helmet": "^6.2.0",
    "http-status-codes": "^2.3.0",
    "mongodb": "^4.13.0",
    "mongodb-memory-server": "^8.16.0",
    "mongoose": "^6.12.1",
    "nodemailer": "^6.9.6",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "vite": "^4.4.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-c8": "^0.27.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "react-router-dom": "^6.16.0",
    "supertest": "^6.3.3",
    "vitest": "^0.27.2"
  }
}
